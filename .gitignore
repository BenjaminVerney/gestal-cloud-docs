# Ignore all logfiles.
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore dependency directories.
node_modules

# Ignore application dist folder.
/dist

# Ignore coverage folder.
/coverage

# Ignore eslint cache.
.eslintcache

# Ignore yarn integrity and clean files.
.yarn-integrity
.yarnclean

# Ignore webpack stats file.
stats.json

# Ignore dotenv file.
.env

# Ignore npm stuff because, you know, yarn.
package-lock.json
